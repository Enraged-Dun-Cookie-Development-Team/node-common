{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "sourceRoot": "./src/",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowJs": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "lib": [ "ESNext", "webworker" ]
  },
}
